        <div class="content-body">

            <div class="container-fluid mt-3">
                <div class="row">
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-1">
                            <div class="card-body">
                                <h3 class="card-title text-white">Registerd Users</h3>
                                <div class="d-inline-block">
                                    <h2 class="text-white"><%=registeredCount%></h2>
                                   
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-shopping-cart"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-2">
                            <div class="card-body">
                                <h3 class="card-title text-white">Matched Users</h3>
                                <div class="d-inline-block">
                                    <h2 class="text-white"><%=matchedCount%></h2>
                
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-money"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-3">
                            <div class="card-body">
                                <h3 class="card-title text-white">Unmatched Users</h3>
                                <div class="d-inline-block">
                                    <h2 class="text-white"><%=unmatchedCount%></h2>
                                   
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-users"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="card gradient-4">
                            <div class="card-body">
                                <h3 class="card-title text-white">Website Viewers</h3>
                                <div class="d-inline-block">
                                    <h2 class="text-white">99</h2>
                                   
                                </div>
                                <span class="float-right display-5 opacity-5"><i class="fa fa-heart"></i></span>
                            </div>
                        </div>
                    </div>
                </div>

                

            

                <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="active-member">
                                    <div class="table-responsive">
                                        <table class="table table-xs mb-0">
                                            <thead>
                                                <tr>
                                                    <th>Name</th>
                                                    <th>Email</th>
                                                    <th>Age</th>
                                                    <th>Gender</th>
                                                    <th>Crush</th>
                                                    <th>Matched</th>
                                                    
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% for(var i=0;i<registeredCount; i++){%>
                                                <tr class="userTableRow" data-href="http://www.google.com">
                                                    <td><img src=<%=users[i].userPhotoLink%> class=" rounded-circle mr-3" alt="">Dasun Nimantha</td>
                                                    <td><%=users[i].userEmail%></td>
                                                    <td>
                                                        <%=users[i].age%>
                                                    </td>
                                                    <td>
                                                        <%=users[i].gender%>
                                                    </td>
                                                    <td><%=users[i].crush%></td>

                                                    <%if (users[i].matched){%>
                                                        <td><i class="fa fa-circle-o text-success  mr-2"></i>Yes</td>
                                                    <%} else {%>
                                                        <td><i class="fa fa-circle-o text-success  mr-2"></i>No</td>
                                                    <%}%>
                                                </tr>
                                                <% } %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>                        
                    </div>
                </div>

                
                

